    ids         = {{"^%d+$"}, "IDs of the detectors", 2},
    modemSide   = {{"^front$", "^back$", "^left$", "^right$", "^top$", "^bottom$", "^modem_%d+$"}, "Modem side"}, 
    monitor     = {{"^front$", "^back$", "^left$", "^right$", "^top$", "^bottom$", "^term$"}, "Monitor side"},
    releaseSide = {{"^left$", "^right$"}, "Release side"},
    relaseSideID= {{"^1$","^2$", ^null$}, "Release side ID (1 = left, 2 = right)"},
  },
